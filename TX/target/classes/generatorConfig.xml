<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="context1">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection
			connectionURL="jdbc:mysql://127.0.0.1:3306/edusys?useUnicode=true&amp;characterEncoding=UTF-8&amp;useSSL=true"
			driverClass="com.mysql.jdbc.Driver" password="root" userId="root" />
		<javaModelGenerator targetPackage="tx.items"
			targetProject="TX" />
		<sqlMapGenerator targetPackage="tx.items"
			targetProject="TX" />
		<javaClientGenerator targetPackage="tx.items"
			targetProject="TX" type="XMLMAPPER" />
		<table schema="person" tableName="Person" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">

		</table>
	</context>
</generatorConfiguration>